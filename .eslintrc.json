{
    "extends": ["airbnb-base", "prettier"],
    "rules": {
        "indent": ["error", 4],
        "no-tabs": "off",
        "no-console": "off",
        "no-unused-vars": "off",
        "prettier/prettier": ["error"],
        "consistent-return": "off",
        "no-shadow": "off",
        "func-names": "off",
        "no-use-before-define": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off"
    },
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "node": true
    }
}
